{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via NPM",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}/tarkov-tracker",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "dev",
      ]
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/tarkov-tracker/src"
    }
  ]
}